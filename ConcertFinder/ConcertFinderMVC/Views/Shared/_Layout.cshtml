    $(document).ready(function () {
        $("#open_connect").click(function () {
            $("#connect_panel").toggle('blind', null, 500);
        });

        $("#SearchBar").keyup(function () {
            if ($(this).val().length >= 2) {
                $.ajax({
                    url: "Event/Search",
                    data: { q: $(this).val() },
                    success: function (data) {
                        for (var i = 0; i < data.length; i++) {
                            alert(data[i]);
                        }
                    },
                    dataType: "json"
                });
            }
        });

    });

</script>